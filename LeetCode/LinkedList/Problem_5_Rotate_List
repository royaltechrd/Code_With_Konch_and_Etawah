#Messy Code but working 

# Definition for singly-linked list.
# class ListNode:
#     def __init__(self, val=0, next=None):
#         self.val = val
#         self.next = next
class Solution:
    def rotateRight(self, head: Optional[ListNode], k: int) -> Optional[ListNode]:
        temp=head
        cur=temp
        
        #Initialise the Counter
        count=0
        if head is None:
            # If the List is empty then return it no purpose of rotating it
            count=0
            return head
        else: 
            count=1
        
        # Finding the length of the Linked List
        while cur.next!=None:
            cur=cur.next
            count+=1
        #Division of length and given int to rotate as we can reduce the iteration
        # as rotate the n times of n length of linked list have no meaning as it will return the same thing
        if k>=count:
            n=k%count
        else:
            n=k
        print(n,count,k)
        cur=temp
        
        #Main Program 
        while n!=0:
            if head.next==None:
                return head
            prev=None
            while cur.next!=None:
                prev=cur
                cur=cur.next
            prev.next=None
            cur.next=temp
            temp=cur
            n=n-1
        return cur
