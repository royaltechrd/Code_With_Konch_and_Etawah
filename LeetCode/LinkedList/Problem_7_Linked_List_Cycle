#Runtime: 2427 ms, faster than 5.01% of Python3 online submissions for Linked List Cycle.
#Memory Usage: 17.6 MB, less than 66.38% of Python3 online submissions for Linked List Cycle.

# Definition for singly-linked list.
# class ListNode:
#     def __init__(self, x):
#         self.val = x
#         self.next = None

class Solution:
    def hasCycle(self, head: Optional[ListNode]) -> bool:
        cur=head
        addrsofPointer=[]
        if cur:
            if cur==cur.next:
                return True 
        else:
            return False
        while cur.next!=None:
            addrsofPointer.append(cur)
            cur=cur.next
            if cur in addrsofPointer:
                return True
        return False
